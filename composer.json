{
  "name": "codedive-io/laravel-ncloud-sdk",
  "description": "네이버 클라우드 SDK",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "codedive.io",
      "email": "foxtrot@codedive.io"
    }
  ],
  "require": {
    "php": "^8.1",
    "laravel/framework": "^12.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^12.3",
    "orchestra/testbench": "^10.6"
  },
  "autoload": {
    "psr-4": {
      "Codedive\\LaravelNcloudSdk\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Codedive\\LaravelNcloudSdk\\Tests\\": "tests/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Codedive\\LaravelNcloudSdk\\LaravelNcloudSdkServiceProvider"
      ],
      "aliases": {
        "OutboundMailer": "CloudOutboundMailer"
      }
    }
  }
}
